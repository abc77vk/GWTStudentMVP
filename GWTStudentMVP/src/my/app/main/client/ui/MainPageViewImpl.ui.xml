<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style>
		.panel {
			background-color: ivory;
		}
		.menu {
		}
	</ui:style>

	<g:HTMLPanel styleName="{style.panel}">
		<g:Grid cellPadding="10" cellSpacing="10">
			<g:row>
				<g:customCell>
					<g:VerticalPanel styleName="{style.menu}" spacing="20">
						<g:Anchor text="Show all Mark" ui:field="anchor"/>
						<g:Anchor text="Add new Mark"/>
					</g:VerticalPanel>
				</g:customCell>
				<g:customCell>
					<g:HTMLPanel ui:field="content">
						<g:Label text="New Label" ui:field="output"/>
					</g:HTMLPanel>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder>
