<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style>
		.panel {
			background-color: ivory;
		}
		.menu {
		}
	</ui:style>

	<g:HTMLPanel styleName="{style.panel}">
		<g:Grid>
			<g:row>
				<g:customCell>
					<g:VerticalPanel styleName="{style.menu}">
						<g:Hyperlink text="Show all Mark" targetHistoryToken="marks"/>
						<g:Hyperlink text="Add new Mark" targetHistoryToken="newmark"/>
					</g:VerticalPanel>
				</g:customCell>
				<g:customCell>
					<g:HTMLPanel ui:field="content"/>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder>
