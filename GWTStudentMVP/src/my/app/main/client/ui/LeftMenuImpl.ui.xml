<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style>
		.panel {
			background-color: transparent;
		}
		.table {
		}
		.{style.table} {
		}
	</ui:style>

	<g:HTMLPanel styleName="{style.panel}">
		<g:VerticalPanel spacing="10">
			<g:Button text="Main page" width="100%" ui:field="button_2"/>
			<g:Button text="Add new Mark" ui:field="button" styleName="gwt-Button" width="100%"/>
			<g:Button text="Show Marks" width="100%" ui:field="button_1"/>
			<g:Button width="100%" ui:field="button_3" text="Log out"></g:Button>
		</g:VerticalPanel>
		<!-- <h2>Menu</h2>
			<ul id="leftnavmenu"> -->
				<g:Grid cellPadding="10" cellSpacing="10" styleName="{style.table}">
					<g:row>
						<g:customCell>
							<g:Label text="Name"/>
						</g:customCell>
						<g:customCell>
							<g:Label ui:field="nameOut"/>
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label text="Age"/>
						</g:customCell>
						<g:customCell>
							<g:Label ui:field="ageOut"/>
						</g:customCell>
					</g:row>
				</g:Grid>
			<!-- </ul> -->
			
	</g:HTMLPanel>
</ui:UiBinder>
